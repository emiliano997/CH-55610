<div>
  <table class="table">
    <thead>
      <tr>
        <th scope="col"> Group </th>
        <th scope="col"> First Name </th>
        <th scope="col"> Last Name </th>
        <th scope="col"> Grade </th>
        <th scope="col"> Gender </th>
        <th scope="col"> Email </th>
      </tr>
    </thead>
    <tbody>
      {{#each students.docs}}
        <tr>
          <td> {{this.group}}</td>
          <td> {{this.first_name}}</td>
          <td> {{this.last_name}}</td>
          <td> {{this.grade}} </td>
          <td> {{this.gender}} </td>
          <td> {{this.email}}</td>
        </tr>
      {{/each}}

    </tbody>

  </table>
 
  </div>
</div>